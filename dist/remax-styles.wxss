.status-bar {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    z-index: 1;
}

.nav-bg {
    width: 100%;
    position: absolute;
    bottom: -64rpx;
    z-index: -1;
}

.nav-bar {
    top: 0;
    position: fixed;
    left: 0;
    width: 100%;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    z-index: 1;
    box-sizing: border-box;
    font-family: 'Microsoft Yahei','微软雅黑','Segoe UI',Tahoma,Geneva,Verdana,sans-serif;
    font-size: 32rpx;
    text-align: center;
}

.msg-container {
    margin-top: 100rpx;
    width: 266rpx;
    height: 132rpx;
    background: rgba(255,190,78,1);
    box-shadow: 0rpx 4rpx 0rpx 0rpx rgba(225,142,33,1);
    border-bottom: 2rpx solid rgba(255,227,179,1);
    border-radius: 0rpx 66rpx 66rpx 0rpx;
    display: flex;
    align-items: center;
    text-align: left;
}

.avatar-container {
    width: 76rpx;
    height: 76rpx;
    border-radius: 50%;
    background: #fff;
    margin: 0 14rpx;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    animation: startText 1.5s infinite;
}

@-webkit-keyframes startText {
    0% {
        transform: scale(1);
    }

    40% {
        transform: scale(1.1);
    }

    80% {
        transform: scale(1);
    }

    100% {
        transform: scale(1);
    }
}

@keyframes startText {
    0% {
        transform: scale(1);
    }

    40% {
        transform: scale(1.1);
    }

    80% {
        transform: scale(1);
    }

    100% {
        transform: scale(1);
    }
}

.avatar {
