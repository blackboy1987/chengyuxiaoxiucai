.game{
    color:red;
}
.game_container{
    background: rgba(172,164,146,1);
    border: 3rpx solid rgba(72,53,38,1);
    border-radius: 20rpx;
    width:97%;
    margin:0 auto;
}

.game_row {
    display: flex;
}

.game_row:first-child {
    margin-top: 2rpx;
}

.game_box {
    box-sizing: border-box;
    margin: 3rpx 3rpx;
    border-radius: 10rpx;
    font-size: 44rpx;
    font-family: MicrosoftYaHei-Bold;
    font-weight: bold;
    color: rgba(72,53,38,1);
    position: relative;
    flex-shrink: 0;
    display: table;
    width:76rpx;
    height:76rpx;
}

.box-text {
    width: 100%;
    height: 100%;
    left: 0;
    top: -1rpx;
    position: absolute;
    display: flex;
    justify-content: center;
    align-items: center;
    line-height: 1;
}

.game_box.space {
    background: #fff;
    border: 2rpx solid rgba(72,53,38,1);
}

.game_box.block {
    background: rgba(252,251,237,1);
    border: 2rpx solid rgba(72,53,38,1);
    box-shadow: 0rpx -6rpx 0rpx 0rpx rgba(72,53,38,1) inset;
    border-radius: 10rpx;
}

.game_box.text {
    background: rgba(252,240,165,1);
    border: 2rpx solid rgba(72,53,38,1);
    box-shadow: 0rpx -6rpx 0rpx 0rpx rgba(72,53,38,1) inset;
    border-radius: 10rpx;
    font-family: MicrosoftYaHei-Bold;
    color: rgba(127,84,72,1);
}

.game_box.active {
    background: rgba(255,255,249,1);
    border: 9rpx solid rgba(255,42,0,1);
    border-radius: 10rpx;
}

.game_box.fail {
    font-family: MicrosoftYaHei-Bold;
    color: rgba(205,53,53,1);
    background: rgba(255,240,170,1);
    box-shadow: 0rpx -6rpx 0rpx 0rpx rgba(72,53,38,1) inset;
    border-radius: 10rpx;
}

.game_box.active.fail {
    box-shadow: 0rpx -6rpx 0rpx 0rpx rgba(72,53,38,1) inset;
}

.game_box.fail-move {
    animation: failMove 0.3s;
}

.game_box.success {
    background: rgba(132,195,72,1);
    border: 1rpx solid rgba(72,53,38,1);
    box-shadow: 0rpx -6rpx 0rpx 0rpx rgba(65,97,40,1) inset;
    border-radius: 10rpx;
    color: #fff;
    animation: successMove 0.5s;
    animation-fill-mode: forwards;
}

.game_box.success2 {
    background: rgba(132,195,72,1);
    border: 1rpx solid rgba(72,53,38,1);
    box-shadow: 0rpx -6rpx 0rpx 0rpx rgba(65,97,40,1) inset;
    border-radius: 10rpx;
    color: #fff;
    animation: successMove2 0.5s;
    animation-fill-mode: forwards;
}

.game_box.animate1 {
    animation-delay: 0;
}

.game_box.animate2 {
    animation-delay: 0.1s;
}

.game_box.animate3 {
    animation-delay: 0.2s;
}

.game_box.animate4 {
    animation-delay: 0.3s;
}

.tool-container {
    width: 710rpx;
    margin: auto;
    display: flex;
    align-items: center;
    margin-top: 5rpx;
    position: relative;
}

.tool-left {
    width: 240rpx;
    height: 80rpx;
    background: rgba(230,226,217,1);
    border: 3rpx solid rgba(72,53,38,1);
    border-radius: 40rpx;
    display: flex;
    align-items: center;
    overflow: hidden;
}

.tool-left-avatar {
    width: 80rpx;
    height: 80rpx;
    border-radius: 40rpx;
    border-left: 0rpx solid rgba(72,53,38,1);
    border-top: 3rpx solid rgba(72,53,38,1);
    border-right: 3rpx solid rgba(72,53,38,1);
    border-bottom: 3rpx solid rgba(72,53,38,1);
    display: block;
    overflow: hidden;
}

.tool-left-content {
    flex: 1;
    text-align: center;
    line-height: 1;
    margin: 10rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.tool-left-tip {
    font-size: 28rpx;
    font-family: MicrosoftYaHei-Bold;
    font-weight: bold;
    color: rgba(72,53,38,1);
    line-height: 26rpx;
}

.tool-left-tip:first-child {
    margin-bottom: 10rpx;
}

.tool-right {
    flex: 1;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.right-icon-container {
    position: relative;
    margin-left: 30rpx;
}
.right-icon {
    width: 80rpx;
    height: 100rpx;
    display: block;
}

.tip-icon {
    width: 170rpx;
    height: 80rpx;
    display: block;
}

.tip-type-box {
    position: absolute;
    width: 44rpx;
    height: 30rpx;
    background: rgba(72,53,38,1);
    border-radius: 15rpx;
    right: 0rpx;
    bottom: 0rpx;
    font-size: 18rpx;
    font-family: MicrosoftYaHei-Bold;
    font-weight: bold;
    color: rgba(255,255,255,1);
    display: flex;
    justify-content: center;
    align-items: center;
    line-height: 18rpx;
}

.select_container {
    width: 97%;
    margin: 8rpx auto 50rpx;
    position: relative;
}
.select-row {
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
}

.select-item{
    margin-bottom: 8rpx;
}

.select_box {
    display: flex;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
    background: #fff;
    width: 86rpx;
    height: 81rpx;
    background: rgba(252,251,237,1);
    border: 2rpx solid rgba(72,53,38,1);
    box-shadow: 0rpx 5rpx 0rpx 0rpx rgba(72,53,38,1);
    border-radius: 10rpx;
    font-size: 60rpx;
    font-family: MicrosoftYaHei-Bold;
    font-weight: bold;
    color: rgba(72,53,38,1);
    transition: all 0.2s;
    transform: scale(1,1);
    position: relative;
}
.big-select1 {
    width: 115rpx;
    height: 115rpx;
}
.big-select2 {
    width: 100rpx;
    height: 100rpx;
}
.select_box.selected {
    opacity: 0;
    transform: scale(0,0);
}
.btn-scale {
    animation: btnScale 0.6s linear;
}
@-webkit-keyframes btnScale {
    0% {
        transform: scale(1);
    }

    25% {
        transform: scale(1.15);
    }

    50% {
        transform: scale(1);
    }

    75% {
        transform: scale(1.15);
    }

    100% {
        transform: scale(1);
    }
}

@keyframes btnScale {
    0% {
        transform: scale(1);
    }

    25% {
        transform: scale(1.15);
    }

    50% {
        transform: scale(1);
    }

    75% {
        transform: scale(1.15);
    }

    100% {
        transform: scale(1);
    }
}

.list-popup-title {
    width: 280rpx;
    height: 74rpx;
    display: block;
    margin: 18rpx auto;
}

.list-scroll-list {
    height: 55vh;
    margin-bottom: 56rpx;
    overflow: scroll;
    -webkit-overflow-scrolling: touch;
}

.list-scroll-item {
    width: 526rpx;
    position: relative;
    margin: 0 auto 20rpx;
}
.list-item-bg {
    width: 526rpx;
    height: 84rpx;
    display: block;
}

.list-item-content {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    line-height: 1;
    box-sizing: border-box;
    padding: 0 40rpx;
}

.list-item-name {
    font-size: 42rpx;
    font-family: MicrosoftYaHei-Bold;
    font-weight: bold;
}

.list-item-round {
    font-size: 28rpx;
    font-family: MicrosoftYaHei-Bold;
    font-weight: bold;
}


.blue .list-item-name {
    color: rgba(2,155,213,1);
}

.blue .list-item-round {
    color: rgba(36,97,121,1);
}

.yellow .list-item-name {
    color: rgba(220,171,48,1);
}

.yellow .list-item-round {
    color: rgba(147,117,42,1);
}

.grey .list-item-name {
    color: rgba(181,184,186,1);
}

.grey .list-item-round {
    color: rgba(181,184,186,1);
}
.list-popup {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 20;
    background: rgba(44,40,36,0.65);
    display: flex;
    justify-content: center;
    align-items: center;
}
.popup-container {
    width: 652rpx;
    background: rgba(253,246,230,1);
    border: 2rpx solid rgba(72,53,38,1);
    box-shadow: 0rpx -14rpx 0rpx 0rpx rgba(224,211,181,1) inset;
    border-radius: 30rpx;
    position: relative;
}
.close-btn-container {
    position: absolute;
    right: 0;
    top: 0;
    width: 120rpx;
    height: 120rpx;
    z-index: 1;
}

.close-btn {
    position: absolute;
    right: 26rpx;
    top: 26rpx;
    width: 60rpx;
    height: 60rpx;
}
