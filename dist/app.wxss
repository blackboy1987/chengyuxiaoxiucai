  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  z-index: 8;
  background: rgba(44,40,36,0.65);
}

.seo-view {
  position: fixed;
  left: 0;
  bottom: 0;
  transform: translateY(200%);
}

.start-text-move {
  animation: startText 1.5s infinite;
}

@-webkit-keyframes startText {
  0% {
    transform: scale(1);
  }

  40% {
    transform: scale(1.1);
  }

  80% {
    transform: scale(1);
  }

  100% {
    transform: scale(1);
  }
}

@keyframes startText {
  0% {
    transform: scale(1);
  }

  40% {
    transform: scale(1.1);
  }

  80% {
    transform: scale(1);
  }

  100% {
    transform: scale(1);
  }
}

@-webkit-keyframes scaleAnimation {
  0% {
    transform: scale(1);
  }

  50% {
    transform: scale(1.4);
  }

  100% {
    transform: scale(1);
  }
}

@keyframes scaleAnimation {
  0% {
    transform: scale(1);
  }

  50% {
    transform: scale(1.4);
  }

  100% {
    transform: scale(1);
  }
}
